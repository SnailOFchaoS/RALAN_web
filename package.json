{
  "name": "ralan_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean:out": "powershell -NoProfile -Command \"If (Test-Path ./out) { Remove-Item -Recurse -Force ./out }\" || exit 0",
    "build:gh-pages": "npm run clean:out && set NEXT_EXPORT=1&& set BASE_PATH=/RALAN_web&& next build",
    "serve:gh-pages": "node scripts/serve-ghpages.cjs",
    "add:nojekyll": "powershell -NoProfile -Command \"New-Item -Path ./out/.nojekyll -ItemType File -Force | Out-Null\"",
    "predeploy": "npm run build:gh-pages && npm run add:nojekyll",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.93.0",
    "swiper": "^12.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "express": "^4.19.2",
    "gh-pages": "^6.3.0",
    "typescript": "^5"
  }
}
